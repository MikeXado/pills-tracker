  "license": "MIT",
  "scripts": {
    "dev": "pnpm run db:up && pnpm run db:migrate:dev && pnpm dlx concurrently --kill-others \"npm:dev:web\" \"npm:dev:server\"",
    "dev:web": "pnpm nx run ulim-web:serve",
    "dev:server": "pnpm nx run ulim-server:serve",
    "------SHADCN------": "------SHADCN------",
    "add-component": "pnpm dlx shadcn-ui@latest add",
    "------DATABASE----": "-----DATABASE-----",
    "db:up": "docker compose -f tools/student-ulim-env/dev.docker-compose.yml up -d --no-recreate --remove-orphans",
    "db:studio": "pnpm dlx env-cmd -f apps/pills-tracker-server/.env prisma studio",
    "db:push": "pnpm dlx env-cmd -f apps/pills-tracker-server/.env prisma db push --accept-data-loss",
    "db:prisma-gen": "pnpm dlx env-cmd -f apps/pills-tracker-server/.env prisma generate",
    "db:migrate:dev": "pnpm dlx env-cmd -f apps/pills-tracker-server/.env prisma migrate dev",
    "db:migrate:prod": "pnpm dlx prisma migrate deploy"
  },
  "private": true,
